		xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

		<bean id="answerMap" class="java.util.HashMap">
			<constructor-arg index="0">
				<map>
					<entry key="kurs" value-ref="answer_pbygg"/>
				</map>
			</constructor-arg>
		</bean>
		
		<bean id="answer_pbygg" class="no.uis.service.fsimport.mock.ResourceAnswer">
			<constructor-arg value="data/kurs_pbygg_b.xml"/>
		</bean>
</beans>
