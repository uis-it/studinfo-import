  <xs:simpleType name="FsTerminkodeYearMonth">
    <xs:annotation>
      <xs:documentation>Format: YYYYMMXX where XX is an optional kode
        kodes: EO - ekstra ordinær, K - kont, KN - Kont ny
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:pattern value="\d{6}(EO|K|KN)?"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="FsSemesterkode">
    <xs:annotation>
      <xs:documentation>Format: YYYY where X either H(øst) or V(år)</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:pattern value="\d{4}(H|V)?"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="FsDate">
    <xs:annotation>
      <xs:documentation>This should be xs:date with proper separators, and "Frist innlevering: should be put in a separate element</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:pattern value="\d{2}\.\d{2}\.\d{4}"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="FsTime">
    <xs:annotation>
      <xs:documentation>This should be formatted as proper xs:time</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:pattern value="(\d{2}:\d{2})?"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="FsEksamensdato">
    <xs:annotation>
      <xs:documentation>This should be a complex type that contains date and time for the test and / or submission date.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:pattern value="((Uttak\: \d{2}\.\d{2}\.\d{4} )?(Frist innlevering\: )?(\d{2}\.\d{2}\.\d{4})( kl\. \d{2}\:\d{2})?)?"></xs:pattern>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="FsNullableDate">
    <xs:annotation>
      <xs:documentation>Allows empty date, see http://objectmix.com/xml-soap/86477-optional-date-element-xsd-little-help-please.html</xs:documentation>
    </xs:annotation>
    <xs:union memberTypes="xs:string">
      <xs:simpleType>
        <xs:restriction base="xs:string">
          <xs:minLength value="0"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:union>
  </xs:simpleType>
  <xs:simpleType name="FsSemester">
    <xs:restriction base="xs:string">
      <xs:pattern value="(HØST|VÅR)"/>
    </xs:restriction>
  </xs:simpleType>
